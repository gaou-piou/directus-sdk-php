{
    "name": "directus/sdk",
    "description": "Directus SDK for PHP",
    "keywords": ["directus", "sdk"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "directus/database": "0.1.*",
        "directus/permissions": "0.9.*",
        "directus/filesystem": "0.9.*",
        "directus/hook": "0.9.*",
        "directus/utils": "0.9.*",
        "guzzlehttp/guzzle": "~5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Directus\\SDK\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/wellingguzman/zend-db"
        }
    ]
}
