{
    "name": "directus/sdk",
    "description": "Directus SDK for PHP",
    "keywords": ["directus", "sdk"],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "directus/database": "dev-d64",
        "directus/acl": "0.9.*",
        "directus/hook": "0.9.*",
        "directus/utils": "0.9.*",
        "guzzlehttp/guzzle": "~5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Directus\\SDK\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/directus/directus-acl"
        },
        {
            "type": "git",
            "url": "https://github.com/directus/directus-hook"
        },
        {
            "type": "git",
            "url": "https://github.com/directus/directus-php-utils"
        }
    ]
}
